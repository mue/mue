{
    "manifest_version": 2,
    "name": "Mue",
    "description": "Fast, open and free-to-use new tab page for most modern browsers.",
    "version": "0.7.1",
    "browser_action": {
        "default_icon": "./icons/extension-icon.png"
    },
    "background": {
        "scripts": [
            "./background-opera.js"
        ]
    },
    "permissions": [
        "tabs"
    ]
}